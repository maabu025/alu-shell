#!/usr/bin/env bash
# Path to the log file
LOG_FILE="/path/to/your/access.log"

# Check if the file exists
if [ -e "$LOG_FILE" ]; then
    # Use awk to group by IP and HTTP status code, and display the data
    awk '{ count[$1" "$9]++ } END { for (entry in count) print count[entry], entry }' "$LOG_FILE" | sort -nr
else
    echo "Error: $LOG_FILE does not exist."
fi
